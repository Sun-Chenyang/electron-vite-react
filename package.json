{
  "name": "electron-vite-react",
  "version": "1.0.0",
  "private": true,
  "main": "build/main/main.js",
  "description": "A simple way to create and manage lists, backed by SQLite",
  "scripts": {
    "start": "node scripts/dev-server.js",
    "test": "playwright test",
    "format": "prettier --write .",
    "format-check": "prettier --check .",
    "build": "node scripts/build.js && electron-builder -c config/electron-builder.json",
    "build:win": "node scripts/build.js && electron-builder -c config/electron-builder.json --win",
    "build:mac": "node scripts/build.js && electron-builder -c config/electron-builder.json --mac",
    "build:linux": "node scripts/build.js && electron-builder -c config/electron-builder.json --linux",
    "build:dev": "node scripts/build.js",
    "serve:renderer": "vite -c config/vite.js preview"
  },
  "repository": "https://github.com/171928131/electron-vite-react",
  "devDependencies": {
    "@playwright/test": "^1.27.1",
    "@quasar/vite-plugin": "^1.2.3",
    "@react-dev-inspector/vite-plugin": "^2.0.0",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@vitejs/plugin-react": "^2.2.0",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "electron": "^21.2.3",
    "electron-builder": "^23.6.0",
    "electron-devtools-installer": "^3.2.0",
    "less": "^4.1.3",
    "playwright": "^1.27.1",
    "prettier": "2.7.1",
    "react-dev-inspector": "^2.0.0",
    "typescript": "^4.8.4",
    "vite": "^4.4.9"
  },
  "dependencies": {
    "@quasar/extras": "^1.15.5",
    "antd": "^4.24.2",
    "electron-updater": "^5.3.0",
    "quasar": "^2.10.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sequelize": "^6.25.5",
    "sqlite3": "^5.1.2"
  }
}
